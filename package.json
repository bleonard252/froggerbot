{
  "name": "froggerbot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node -r ts-node/register src/build.ts",
    "dev:gateway": "node -r ts-node/register src/build.ts && node -r ts-node/register --inspect src/index.ts",
    "dev:gateway:watch": "npm run build && tsc-watch --project . --outDir ./dist --onSuccess \"nodemon --exec 'npm run dev:gateway'\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bleonard252/frogger.git"
  },
  "keywords": [
    "discord-bot"
  ],
  "author": "Blake Leonard <blake@1024256.xyz>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bleonard252/frogger/issues"
  },
  "homepage": "https://github.com/bleonard252/frogger#readme",
  "dependencies": {
    "@discordjs/builders": "^0.12.0",
    "@discordjs/rest": "^0.3.0",
    "chalk": "^4.1.0",
    "discord.js": "^13.6.0",
    "dotenv": "^16.0.0",
    "filehound": "^1.17.5",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "ts-node": "^10.5.0"
  }
}
